<%unless wiki.id.nil?%>
      <tr>   
        <td>
          <p><%= link_to wiki.title, wiki_path(wiki.id) %></p>
        </td>
        <td>
          <%= link_to "Edit", edit_wiki_path(wiki.id) %>
        </td>
        <td>
          <%= link_to "Delete", wiki_path(wiki), method: :delete, data: { confirm: "Are you sure you want to delete this Wiki?" } %>
        </td>
      </tr>
<%end%>